frontend http-in
    bind 0.0.0.0:80
    mode http
    acl is-grafana  path_beg /grafana/
    acl is-prometheus  path_beg /prometheus/
    use_backend grafana-backend if  is-grafana
    use_backend prometheus-backend if  is-prometheus

backend grafana-backend
    server node1 grafana:3000
    reqrep ^([^\ ]*\ )/prometheus(.*)     \1/\2
    mode http


backend prometheus-backend
    server node1 prometheus:9090
    reqrep ^([^\ ]*\ )/prometheus(.*)     \1/\2
    mode http
